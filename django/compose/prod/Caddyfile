:80
{

    encode zstd gzip

    @statics {
        path /static/*
        path /media/*
    }

    handle @statics {
        root * /data/
        file_server
    }

    handle /broker {
        reverse_proxy  mqtt_02_prod:9001
    }

    handle /sock/* {
        reverse_proxy django_02_prod:9000
    }

    handle {
        reverse_proxy  django_02_prod:8000
    }

}